      <dependency id="Microsoft.Data.OData" version="[5.6.3]" />
    </dependencies>
    <frameworkAssemblies>
      <frameworkAssembly assemblyName="System.Windows" targetFramework="Silverlight4.0" />
      <frameworkAssembly assemblyName="System.Xml.Linq" targetFramework="Silverlight4.0" />
    </frameworkAssemblies>
    <references>
      <reference file="Microsoft.Data.Services.Client.dll" />
    </references>
  </metadata>
